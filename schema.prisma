generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("PG_CONNECTION_STRING")
}

model users {
  id       Int     @id @default(autoincrement())
  username String @unique()
  password String?
  age Int?
  posts    posts[]
}

model posts {
  id      Int    @id @default(autoincrement())
  post    String
  user_id Int?
  users   users? @relation(fields: [user_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
}
